<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.edu.curtin.assignmentmad">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Market" android:label="Market Activity" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Overview" android:label="Overview Activity" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".SmellOScope" android:label="Smell Activity" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Wilderness" android:label="Wildness Activity" android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>